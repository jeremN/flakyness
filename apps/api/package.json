{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=../../.env src/index.ts",
    "build": "tsup src/index.ts --format esm",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx --env-file=../../.env node_modules/drizzle-kit/bin.cjs migrate",
    "db:push": "tsx --env-file=../../.env node_modules/drizzle-kit/bin.cjs push",
    "db:studio": "tsx --env-file=../../.env node_modules/drizzle-kit/bin.cjs studio",
    "db:seed": "tsx --env-file=../../.env src/db/seed.ts",
    "simulate:ci": "tsx --env-file=../../.env scripts/simulate-ci.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.0",
    "@hono/zod-validator": "^0.7.5",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.36.0",
    "hono": "^4.6.0",
    "hono-rate-limiter": "^0.4.2",
    "postgres": "^3.4.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "drizzle-kit": "^0.28.0",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
